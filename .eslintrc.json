{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
      "camelcase": "error",
      "spaced-comment": "error",
      "quotes": ["error", "single"],
      "no-duplicate-imports": "error",
      "no-multi-spaces": ["error"],
      "semi": "error",
      "linebreak-style": ["error", "windows"]
    }
  }